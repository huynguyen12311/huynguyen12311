	Name = "Auto Farm",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local PlayerTab = Window:MakeTab({
	Name = "Player",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local OthersTab = Window:MakeTab({
	Name = "Others Farm",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
